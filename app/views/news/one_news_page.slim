.container
  .row
    .news-dirs.gap
      i.department-news.post 
        = "Новости кафедры"
      div style=("border-left:2px solid #000; display: inline; margin: 5px;")
      a.news-dir-a ng-href="#/news"
        .news-dir 
          = "К списку новостей"
      div style=("border-left:2px solid #000; display: inline; margin: 5px;")
      a.news-dir-a ng-href="#/news/page"
        .news-dir 
          = "К следующей"
      div style=("border-left:2px solid #000; display: inline; margin: 5px;")
      a.news-dir-a ng-href="#/news/page" 
        .news-dir 
          = "К предыдущей"
    div ng-repeat="oneNews in news | orderBy: '-id'"
      i.time.time-inside-post.gap  
        = "{{oneNews.createdAt}}"
      .post-title-inside-post.gap  
        = "{{oneNews.title}}"
      br
      img.full-post-img.gap alt=("Cinque Terre") ng-src="{{oneNews.img}}" 
